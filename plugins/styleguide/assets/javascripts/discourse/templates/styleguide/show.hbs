<StyleguideSection @section={{this.section}}>
  {{#if this.note}}
    <div class="styleguide-note">
      {{component (concat "notes/" this.note)}}
    </div>
  {{/if}}

  {{component
    (concat "styleguide/" this.section.templateName)
    dummy=this.dummy
    dummyAction=this.dummyAction
  }}
</StyleguideSection>