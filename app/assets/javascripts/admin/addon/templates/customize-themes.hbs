<PluginOutlet @name="admin-customize-themes">
  {{#unless this.editingTheme}}
    <ThemesList
      @themes={{this.fullThemes}}
      @components={{this.childThemes}}
      @currentTab={{this.currentTab}}
    />
    <ThemesGrid
      @themes={{this.fullThemes}}
      @components={{this.childThemes}}
      @installModal={{route-action "installModal"}}
    />
  {{/unless}}

  {{outlet}}
</PluginOutlet>