<div class="sidebar-section-link-wrapper">
  <DButton
    class="sidebar-section-link sidebar-row --link-button"
    @noAutoClass={{true}}
    @action={{@action}}
  >
    <span class="sidebar-section-link-prefix icon">
      {{d-icon @icon}}
    </span>
    <span class="sidebar-section-link-content-text">
      {{@text}}
    </span>
  </DButton>
</div>