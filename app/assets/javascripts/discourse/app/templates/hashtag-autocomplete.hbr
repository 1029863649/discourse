<div class='autocomplete hashtag-autocomplete'>
  <ul>
    {{#each options as |option|}}
      <li class="hashtag-autocomplete__option">
        <a href style="color: #{{option.color}}">{{d-icon option.icon}}<span class="hashtag-autocomplete__text">{{option.text}}</span></a>
      </li>
    {{/each}}
  </ul>
</div>
