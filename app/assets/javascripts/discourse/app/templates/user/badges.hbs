{{#d-section pageClass="user-badges" class="user-content user-badges-list"}}
  {{#each sortedBadges as |ub|}}
    {{badge-card
      badge=ub.badge
      count=ub.count
      username=username
      isFavorite=ub.is_favorite
      onClick=(action "favorite" ub)
      filterUser="true"}}
  {{/each}}
{{/d-section}}
