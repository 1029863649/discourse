<div class="column categories">
  <CategoriesOnly @categories={{this.categories}} />
</div>

<div class="column">
  <CategoriesTopicList @topics={{this.topics}} @filter="latest" @class="latest-topic-list" @orderTopicsBy={{this.orderTopicsBy}} />
</div>

<PluginOutlet @name="extra-categories-column" @connectorTagName="div" />
