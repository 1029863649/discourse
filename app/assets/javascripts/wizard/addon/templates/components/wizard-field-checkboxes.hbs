{{#each field.choices as |c|}}
  <div class="checkbox-field-choice {{fieldClass}}">
    <label id={{c.id}} value={{c.label}}>
      <Input @type="checkbox" class="wizard-checkbox" @checked={{c.checked}} {{on "click" (action "changed")}} />
      {{#if c.icon}}
        {{d-icon c.icon}}
      {{/if}}
      {{c.label}}
    </label>
  </div>
{{/each}}
