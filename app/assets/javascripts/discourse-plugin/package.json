{
  "name": "discourse-plugin",
  "version": "1.0.0",
  "private": true,
  "description": "Discourse's plugin API",
  "author": "Discourse <team@discourse.org>",
  "license": "GPL-2.0-only",
  "keywords": [
    "ember-addon"
  ],
  "exports": {
    ".": "./src/index.js",
    "./helpers/*": "./src/helpers/*.js",
    "./services": "./src/services/index.js",
    "./services/*": "./src/services/*.js",
    "./utils/*": "./src/utils/*.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "src"
  ],
  "dependencies": {
    "@embroider/addon-shim": "^1.0.0",
    "ember-auto-import": "^2.6.3"
  },
  "peerDependencies": {
    "discourse-common": "1.0.0",
    "discourse-i18n": "1.0.0",
    "ember-polaris-service": "^0.5.0"
  },
  "engines": {
    "node": "16.* || >= 18",
    "npm": "please-use-yarn",
    "yarn": ">= 1.21.1"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {},
    "externals": ["discourse/lib/ajax"]
  }
}
